{
  "name": "sreg-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "docdash": "^1.2.0",
    "eslint": "^8.6.0",
    "jest": "^27.4.7",
    "jsdoc": "^3.6.7",
    "modal": "^1.2.0",
    "node-sass": "^6.0.1",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-modal": "^3.14.4",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.0",
    "react-select": "^5.2.2",
    "react-test-renderer": "^17.0.2",
    "regenerator-runtime": "^0.13.9",
    "web-vitals": "^2.1.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "test-cov": "jest --coverage",
    "eject": "react-scripts eject",
    "lint": "eslint \"src/**/*.{js,jsx}\"",
    "lint:fix": "eslint \"src/**/*.{js,jsx}\" --fix",
    "prettify": "prettier --write ./src",
    "docs": "node_modules/.bin/jsdoc -c jsdoc.json",
    "dev": "npm run prettify && npm run docs && npm run test-cov && npm run lint"
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "automock": false,
    "setupFiles": [
      "./setupJest.js"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.8",
    "babel-jest": "^27.4.6",
    "jest-fetch-mock": "^3.0.3"
  }
}
